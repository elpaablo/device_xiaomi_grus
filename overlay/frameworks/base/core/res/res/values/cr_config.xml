<resources>

    <!-- Whether devices suports in-display fingerprint when screen is off -->
    <bool name="config_supportScreenOffUdfps">true</bool>

    <!-- Whether device supports sensor based doze customizations -->
    <bool name="config_dozePulseTilt">false</bool>
    <bool name="config_dozePulseProximity">false</bool>
    <bool name="config_deviceHaveWakeUpProximity">false</bool>

    <!-- Whether doze feature is enabled by default in settings -->
    <bool name="config_doze_enabled_by_default">false</bool>

    <!-- Whether the device supports Smart Pixels -->
    <bool name="config_supportSmartPixels">true</bool>

    <!-- Smart charging -->
    <bool name="config_smartChargingAvailable">true</bool>
    <integer name="config_smartChargingBatteryLevel">80</integer>
    <integer name="config_smartChargingBatteryResumeLevel">60</integer>
    <string name="config_smartChargingSysfsNode" translatable="false">/sys/class/power_supply/battery/input_suspend</string>
    <string name="config_smartChargingSuspendValue" translatable="false">1</string>
    <string name="config_smartChargingResumeValue" translatable="false">0</string>

    <!-- Default FOD pressed color -->
    <integer name="config_udfps_pressed_color">3</integer>

    <!-- Pocket Lock -->
    <bool name="config_pocketModeSupported">false</bool>
    <bool name="config_pocketUseLightSensor">false</bool>

</resources>
